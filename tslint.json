{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": false,
        "quotemark": [true, "single", "jsx-double"],
        "no-console": false,
        "arrow-return-shorthand": true,
        "arrow-parens": true,
        "object-literal-sort-keys": false,
        "only-arrow-functions": [true, "allow-declarations", "allow-named-functions"]
    },
    "rulesDirectory": []
}